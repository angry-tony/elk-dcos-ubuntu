{
  "id": "/elasticsearch",
  "volumes": null,
  "cmd": null,
  "args": [
    "--zookeeperMesosUrl",
    "zk://master.mesos:2181/mesos",
    "--zookeeperMesosTimeout",
    "20000",
    "--webUiPort",
    "31105",
    "--frameworkFailoverTimeout",
    "2592000.0",
    "--frameworkName",
    "elasticsearch",
    "--elasticsearchNodes",
    "1",
    "--elasticsearchClusterName",
    "mesos-ha",
    "--elasticsearchCpu",
    "1.0",
    "--elasticsearchDisk",
    "1024",
    "--elasticsearchRam",
    "2048",
    "--executorForcePullImage",
    "false",
    "--executorName",
    "elasticsearch-executor",
    "--elasticsearchDockerImage",
    "elasticsearch:2.4.1"
  ],
  "user": null,
  "env": {
    "JAVA_OPTS": "-Xms128m -Xmx256m"
  },
  "instances": 1,
  "cpus": 0.2,
  "mem": 512,
  "disk": 0,
  "gpus": 0,
  "executor": null,
  "constraints": null,
  "fetch": null,
  "storeUrls": null,
  "backoffSeconds": 1,
  "backoffFactor": 1.15,
  "maxLaunchDelaySeconds": 3600,
  "container": {
    "docker": {
      "image": "mesos/elasticsearch-scheduler:1.0.1",
      "forcePullImage": false,
      "privileged": false,
      "network": "HOST"
    }
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "path": "/",
      "gracePeriodSeconds": 120,
      "intervalSeconds": 30,
      "timeoutSeconds": 5,
      "ignoreHttp1xx": false
    }
  ],
  "readinessChecks": null,
  "dependencies": null,
  "upgradeStrategy": {
    "minimumHealthCapacity": 1,
    "maximumOverCapacity": 1
  },
  "labels": {
    "DCOS_PACKAGE_RELEASE": "2",
    "DCOS_PACKAGE_SOURCE": "https://universe.mesosphere.com/repo",
    "DCOS_PACKAGE_METADATA": "",
    "DCOS_PACKAGE_REGISTRY_VERSION": "3.0",
    "DCOS_PACKAGE_FRAMEWORK_NAME": "elasticsearch",
    "DCOS_PACKAGE_VERSION": "1.0.1-2",
    "DCOS_PACKAGE_NAME": "elasticsearch",
    "DCOS_PACKAGE_IS_FRAMEWORK": "true"
  },
  "acceptedResourceRoles": null,
  "residency": null,
  "secrets": null,
  "taskKillGracePeriodSeconds": null,
  "portDefinitions": [
    {
      "protocol": "tcp",
      "port": 31105
    }
  ],
  "requirePorts": true
}
